ter>\r\nError: {error.message}\r\n</CardFooter>)\r\nreturn (\r\n<CardFooter>\r\n<Button\r\nonClick={()=>{\r\n// alert(JSON.stringify(props.data));\r\naddUser();\r\n}}\r\ndisabled={!props.formValid}\r\n>Sign Up now</Button>\r\n<Button\r\nonClick={() => History.push('/')}\r\n>Cancel</Button>\r\n<Button onClick={()=>History.push('/login')}>Login</Button>\r\n</CardFooter>\r\n)\r\n};//end;\r\n\r\nclass SignUpPage extends Component {\r\nconstructor(props) {\r\nsuper(props);\r\nthis.state = {\r\nerror: null,\r\nisFormValid: true,\r\nisSubmit: false,\r\nuser: {\r\nfirstName: '',\r\nlastName: '',\r\nemail: '',\r\npassword: '',\r\nrePassword: ''\r\n}\r\n};//endState;\r\nthis.ChangeHandel = this.ChangeHandel.bind(this);\r\nthis.validation = this.validation.bind(this);\r\n};//endConstructor;\r\n\r\nChangeHandel(e) {\r\nconst {type, name, value } = e.target;\r\nconst user = this.state.user;\r\nuser[name] = value;\r\nthis.setState({ user })\r\n}\r\nvalidation(type, val) {\r\nconst { user } = this.state;\r\n\r\nif(type==='text' && /^([a-zA-Z]+)$/.test(val)){\r\nalert('value is done')\r\n};//endIf;\r\n}\r\n\r\nrender() {\r\nconst {user,  firstName, lastName, email, password, rePassword } = this.state.user;\r\nreturn (\r\n<Card className=\"w-50 h-100\">\r\n<CardHeader>\r\n<h3>Sign Up</h3>\r\n</CardHeader>\r\n<CardBody>\r\n<Form>\r\n<FormGroup inline>\r\n<Label for=\"fName\">First name</Label>\r\n<Input type=\"text\" name=\"firstName\" id=\"fName\" placeholder=\"first name\"\r\nonChange={this.ChangeHandel}\r\nvalue={firstName}\r\n/>\r\n</FormGroup>\r\n<FormGroup>\r\n<Label for=\"lName\">Last name</Label>\r\n<Input type=\"text\" name=\"lastName\" id=\"lName\" placeholder=\"last name\"\r\nonChange={this.ChangeHandel}\r\nvalue={lastName}\r\n/>\r\n</FormGroup>\r\n<FormGroup>\r\n<Label for=\"Email\">Email</Label>\r\n<Input type=\"email\" name=\"email\" id=\"Email\" placeholder=\"enter email id\"\r\nonChange={this.ChangeHandel}\r\nvalue={email}\r\n/>\r\n</FormGroup>\r\n<FormGroup>\r\n<Labe